<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	tools:context="jp.kght6123.smallappbrowser.activity.SmallBrowserApplicationTestActivity"
	android:background="@drawable/gradation">

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:gravity="fill_horizontal"
		android:padding="20dp"
		android:background="@android:color/background_light"
		android:id="@+id/buttonGroup">
		<Button
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/multi_window_start_button"
			android:text="@string/start" />
		<Button
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/multi_window_open_button"
			android:text="@string/open" />
		<Button
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/multi_window_update_button"
			android:text="@string/update" />
		<Button
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/multi_window_close_button"
			android:text="@string/close" />
		<Button
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/multi_window_exit_button"
			android:text="@string/exit" />
		<Button
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/multi_window_widget_button"
			android:text="@string/widget" />
	</LinearLayout>

</LinearLayout>
